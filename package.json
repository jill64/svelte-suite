{
  "name": "@jill64/svelte-suite",
  "description": "üç∞ Library Suite for quickly building SvelteKit Apps",
  "version": "1.1.40",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "intents.turrets0h@icloud.com",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-suite/issues",
  "homepage": "https://github.com/jill64/svelte-suite#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./icons": {
      "types": "./dist/icons.d.ts",
      "default": "./dist/icons.js"
    },
    "./input": {
      "types": "./dist/input.d.ts",
      "default": "./dist/input.js"
    },
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-suite.git",
    "image": "https://opengraph.githubassets.com/2fba479ca3daecacd612d0e4d3f925e06e79b505162539bc123615e9e34101a6/jill64/svelte-suite"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "library",
    "suite",
    "svelte",
    "sveltekit"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w ."
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.0.12",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@jill64/svelte-dark-theme": "2.3.13",
    "@jill64/svelte-i18n": "1.0.7",
    "@jill64/svelte-input": "1.6.7",
    "@jill64/svelte-menu": "1.0.5",
    "@jill64/svelte-ogp": "1.1.4",
    "@jill64/svelte-storage": "1.1.3",
    "@jill64/svelte-toast": "1.3.7",
    "svelte-feather-icons": "4.1.0",
    "svelte-inline-modal": "1.1.4",
    "svelte-qparam": "1.0.5",
    "ts-serde": "1.0.2"
  }
}