{
  "name": "@jill64/svelte-suite",
  "description": "üç∞ Library Suite for quickly building SvelteKit Apps",
  "version": "1.2.113",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "intents.turrets0h@icloud.com",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-suite/issues",
  "homepage": "https://github.com/jill64/svelte-suite#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./icons": {
      "types": "./dist/icons.d.ts",
      "default": "./dist/icons.js"
    },
    "./input": {
      "types": "./dist/input.d.ts",
      "default": "./dist/input.js"
    },
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    "./i18n/app": {
      "types": "./dist/i18nApp.d.ts",
      "default": "./dist/i18nApp.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "default": "./dist/i18n.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-suite.git",
    "image": "https://opengraph.githubassets.com/17e1f4cca4eb6be32d0164c46bfe017ba4e45abace256cecc221d70803e8466d/jill64/svelte-suite"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "library",
    "suite",
    "svelte",
    "sveltekit"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w ."
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.1.18",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.4.5"
  },
  "dependencies": {
    "@jill64/svelte-dark-theme": "2.3.62",
    "@jill64/svelte-i18n": "1.1.23",
    "@jill64/svelte-input": "1.6.25",
    "@jill64/svelte-menu": "1.0.19",
    "@jill64/svelte-ogp": "1.1.21",
    "@jill64/svelte-storage": "1.1.19",
    "@jill64/svelte-toast": "1.3.33",
    "svelte-feather-icons": "4.1.0",
    "svelte-inline-modal": "1.1.15",
    "svelte-qparam": "1.0.21",
    "ts-serde": "1.0.4"
  }
}
