{
  "name": "@jill64/svelte-suite",
  "description": "üç∞ Library Suite for quickly building SvelteKit Apps",
  "version": "1.0.2",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/svelte-suite/issues",
  "homepage": "https://github.com/jill64/svelte-suite#readme",
  "files": [
    "dist",
    "!*.test.*",
    "!*.spec.*"
  ],
  "exports": {
    "./icons": {
      "types": "./dist/icons.d.ts",
      "default": "./dist/icons.js"
    },
    "./input": {
      "types": "./dist/input.d.ts",
      "default": "./dist/input.js"
    },
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/svelte-suite.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "suite",
    "svelte",
    "sveltekit"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w ."
  },
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-ts": "1.0.1",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@jill64/svelte-dark-theme": "2.2.14",
    "@jill64/svelte-i18n": "0.2.6",
    "@jill64/svelte-input": "1.5.2",
    "@jill64/svelte-menu": "0.0.3",
    "@jill64/svelte-ogp": "0.1.0",
    "@jill64/svelte-storage": "0.0.5",
    "@jill64/svelte-toast": "1.2.5",
    "svelte-feather-icons": "4.0.1",
    "svelte-inline-modal": "1.1.0",
    "svelte-qparam": "0.2.1",
    "ts-serde": "1.0.0"
  }
}
