{
  "name": "@jill64/svelte-suite",
  "description": "üç∞ Library Suite for quickly building SvelteKit Apps",
  "version": "2.1.1",
  "main": "dist/index.js",
  "type": "module",
  "license": "MIT",
  "author": {
    "name": "jill64",
    "email": "contact@jill64.dev",
    "url": "https://github.com/jill64",
    "image": "https://avatars.githubusercontent.com/u/143883742?v=4"
  },
  "bugs": "https://github.com/jill64/svelte-suite/issues",
  "homepage": "https://github.com/jill64/svelte-suite#readme",
  "files": [
    "dist"
  ],
  "exports": {
    "./icons": {
      "types": "./dist/icons.d.ts",
      "default": "./dist/icons.js"
    },
    "./input": {
      "types": "./dist/input.d.ts",
      "default": "./dist/input.js"
    },
    "./serde": {
      "types": "./dist/serde.d.ts",
      "default": "./dist/serde.js"
    },
    "./i18n/app": {
      "types": "./dist/i18nApp.d.ts",
      "default": "./dist/i18nApp.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "default": "./dist/i18n.js"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jill64/svelte-suite.git",
    "image": "https://opengraph.githubassets.com/a4b4f545e08ed9fb5c61cb81b70f3288d51e1b29fbfb10c6c33ac361471d03ec/jill64/svelte-suite"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "library",
    "suite",
    "svelte",
    "sveltekit"
  ],
  "prettier": "@jill64/prettier-config",
  "scripts": {
    "build": "tsc && npx publint",
    "prepack": "npm run build",
    "lint": "npx eslint . && npx depcheck",
    "format": "npx prettier -w ."
  },
  "peerDependencies": {
    "@sveltejs/kit": "^2.0.0",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@jill64/eslint-config-ts": "2.0.8",
    "@jill64/prettier-config": "1.0.0",
    "typescript": "5.7.3"
  },
  "dependencies": {
    "@jill64/svelte-dark-theme": "5.1.2",
    "@jill64/svelte-i18n": "2.2.0",
    "@jill64/svelte-input": "2.0.2",
    "@jill64/svelte-menu": "2.0.0",
    "@jill64/svelte-ogp": "2.0.2",
    "@jill64/svelte-storage": "2.0.0",
    "@jill64/svelte-toast": "2.1.6",
    "svelte-feather-icons": "4.2.0",
    "svelte-inline-modal": "2.0.1",
    "svelte-qparam": "2.1.1",
    "ts-serde": "1.0.8"
  }
}
